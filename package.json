{
  "name": "tastee-ui",
  "version": "1.0.0",
  "description": "Toast your Application with Simple Tests End to End - Ui for Tastee ",
  "main": "main.js",
  "scripts": {
    "start": "./node_modules/electron-prebuilt/dist/Electron.app/Contents/MacOS/Electron .",
    "watch": "./node_modules/.bin/webpack-dev-server",
    "electron-rebuild": "./node_modules/.bin/electron-rebuild",
    "test": "jest --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tastee/tastee-ui.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tastee/tastee-ui/issues"
  },
  "jest": {
    "scriptPreprocessor": "node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "node_modules/react",
      "node_modules/react-dom",
      "node_modules/react-addons-test-utils",
      "node_modules/fbjs"
    ],
    "testDirectoryName": "tests",
    "testPathIgnorePatterns": [
      "node_modules",
      "spec/support"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "react"
    ]
  },
  "homepage": "https://github.com/tastee/tastee-ui#readme",
  "dependencies": {
    "electron-prebuilt": "^1.2.1",
    "react": "^15.1.0",
    "react-bootstrap": "^0.29.4",
    "react-dom": "^15.1.0",
    "react-select": "^1.0.0-beta13",
    "tastee-core": "git+https://github.com/tastee/tastee-core.git"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.9.1",
    "babel-jest": "^11.0.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-jest": "^12.1.0",
    "babel-preset-react": "^6.5.0",
    "css-loader": "^0.23.1",
    "electron-connect": "^0.4.2",
    "fbjs": "^0.8.3",
    "file-loader": "^0.8.5",
    "gulp": "^3.9.1",
    "gulp-codacy": "^1.0.0",
    "jest": "^12.1.1",
    "jest-cli": "^12.1.1",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "mock-require": "^1.3.0",
    "react-addons-test-utils": "^15.1.0",
    "style-loader": "^0.13.1",
    "svg-loader": "0.0.2",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-stream": "^3.2.0"
  }
}
